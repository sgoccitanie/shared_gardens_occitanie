    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/doctrine-orm": "^4.2.12",
    "api-platform/symfony": "^4.2.12",
    "doctrine/dbal": "^3.10.4",
    "doctrine/doctrine-bundle": "^2.18.2",
    "doctrine/doctrine-migrations-bundle": "^3.7.0",
    "doctrine/orm": "^3.6.1",
    "easycorp/easyadmin-bundle": "^4.27.6",
    "edsdk/flmngr-server-php": "^1.6.7",
    "emileperron/tinymce-bundle": ">=3.3",
    "friendsofsymfony/ckeditor-bundle": "^2.7.1",
    "google/apiclient": ">=2.19.0",
    "helios-ag/fm-elfinder-bundle": ">=12.6",
    "league/oauth2-server-bundle": "^0.9.1",
    "nelmio/cors-bundle": "^2.6.1",
    "phpdocumentor/reflection-docblock": "^5.6.6",
    "phpstan/phpdoc-parser": "^1.33",
    "setono/editorjs-bundle": "^1.3",
    "stinger/ckeditor-uploadimage-bundle": "^4.5.6",
    "stof/doctrine-extensions-bundle": "^1.15.2",
    "symfony/asset": "7.1.*",
    "symfony/asset-mapper": "7.1.*",
    "symfony/console": "7.1.*",
    "symfony/doctrine-messenger": "7.1.*",
    "symfony/dom-crawler": "7.1.*",
    "symfony/dotenv": "7.1.*",
    "symfony/expression-language": "7.1.*",
    "symfony/flex": "^2.10.0",
    "symfony/form": "7.1.*",
    "symfony/framework-bundle": "7.1.*",
    "symfony/http-client": "7.1.*",
    "symfony/http-foundation": "^7.4.3",
    "symfony/http-kernel": "7.1",
    "symfony/intl": "7.1.*",
    "symfony/mailer": "7.1.*",
    "symfony/mime": "7.1.*",
    "symfony/monolog-bundle": "^3.11.1",
    "symfony/notifier": "7.1.*",
    "symfony/process": "7.1.*",
    "symfony/property-access": "7.1.*",
    "symfony/property-info": "7.1.*",
    "symfony/runtime": "7.1.*",
    "symfony/security-bundle": "7.1.*",
    "symfony/serializer": "7.1.*",
    "symfony/stimulus-bundle": "^2.32.0",
    "symfony/string": "7.1.*",
    "symfony/translation": "7.1.*",
    "symfony/twig-bundle": "7.1.*",
    "symfony/ux-chartjs": ">=2.32.0",
    "symfony/ux-turbo": "^2.32.0",
    "symfony/validator": "7.1.*",
    "symfony/web-link": "7.1.*",
    "symfony/yaml": "7.1.*",
    "symfonycasts/verify-email-bundle": ">=1.18",
    "tattali/calendar-bundle": "^7.0.2",
    "tinymce/tinymce": "^7.9.1",
    "twbs/bootstrap": "^5.3.8",
    "twig/extra-bundle": "^2.12|^3.22.2",
    "twig/intl-extra": "^3.22.1",
    "twig/twig": "^2.12|^3.22.2"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd",
      "elfinder:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "security": {
        "ignore": [
          "PKSA-365x-2zjk-pt47",
          "PKSA-b35n-565h-rs4q"
        ]
      }
    }
  },
  "google/apiclient-services": [
    "Calendar",
    "Drive"
  ],
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "fakerphp/faker": "^1.24",
    "phpunit/phpunit": "^9.6.31",
    "symfony/browser-kit": "7.1.*",
    "symfony/css-selector": "7.1.*",
    "symfony/debug-bundle": "7.1.*",
    "symfony/google-mailer": "7.1.*",
    "symfony/maker-bundle": "^1.65.1",
    "symfony/phpunit-bridge": "^7.4.3",
    "symfony/stopwatch": "7.1.*",
    "symfony/web-profiler-bundle": "7.1.*"
  }
}
